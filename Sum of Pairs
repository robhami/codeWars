var sum_pairs=function(ints, s){
    console.log("ints: ", ints);
    console.log("s: ", s);
    let len = ints.length;
    let maxSpace = len;
    let lhs=len;
    let answerArray=undefined;
    let uniqueInts=[];
    let uints=false;
  
  for (i=0; i<len; i++){
    let pos=i;
    console.log("pos: ", pos);
    let firstNum=ints[pos];
    console.log("first: ", firstNum);
     
    function inclFunc () {
         uints=uniqueInts.includes(firstNum);
         console.log("unints: ", uints);
    }
    
   inclFunc();
      
   if(!uints) {
      uniqueInts.push(firstNum);
      console.log("uniqueInts: ", uniqueInts);    
    
    
        for (j=0; j<len; j++) {
          let firstNum=ints[pos];
          let secPos=j+1+pos;
//           console.log("secPos: ", secPos);    
//           console.log("secPos-pos: ",secPos-pos);     
          let secondNum=(ints[secPos]);
             console.log("second: ", secondNum); 
          let sumVar = firstNum + secondNum;
      //       console.log("sum: ", sumVar);
      //       console.log("j-i: ", j-i)
            if(sumVar===s && secPos-pos<maxSpace)  {
              maxSpace=secPos-pos;
              lhs=pos;
              console.log("Result is: ", firstNum, " ", secondNum);
              console.log("Spacing is: ", maxSpace);
              answerArray=[firstNum, secondNum];
              
            } 
          
          }
      }
  }
  return (answerArray); 
}
